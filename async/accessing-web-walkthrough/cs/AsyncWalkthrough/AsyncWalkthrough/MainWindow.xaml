<Window x:Class="AsyncWalkthrough.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AsyncWalkthrough"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="startButton" Content="Start" Margin="217,19,217,0" VerticalAlignment="Top" Click="startButton_Click" FontWeight="Bold" />
        <TextBox x:Name="resultsTextBox" Margin="1,73,1,1" TextWrapping="Wrap" VerticalAlignment="Top" Height="250" FontFamily="Lucida Console" VerticalScrollBarVisibility="Auto"/>
    </Grid>
</Window>
